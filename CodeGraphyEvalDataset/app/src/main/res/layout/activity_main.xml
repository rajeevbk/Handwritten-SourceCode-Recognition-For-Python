            app:layout_column="0"
            app:layout_row="2" />

        <Button
            android:id="@+id/button11"
            android:layout_width="103dp"
            android:layout_height="wrap_content"
            android:onClick="setMartin"
            android:text="Martin"
            app:layout_column="1"
            app:layout_row="2" />

        <Button
            android:id="@+id/button15"
            android:layout_width="116dp"
            android:layout_height="wrap_content"
            android:onClick="setMicayla"
            android:text="Micayla"
            app:layout_column="2"
            app:layout_row="2" />

        <Button
            android:id="@+id/button16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="setMikel"
            android:text="Mikel"
            app:layout_column="2"
            app:layout_row="4" />

        <Button
            android:id="@+id/button17"
