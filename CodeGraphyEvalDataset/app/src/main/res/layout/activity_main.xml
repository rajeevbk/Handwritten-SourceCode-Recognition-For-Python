            android:layout_height="wrap_content"
            android:onClick="setJoseph"
            android:text="Joseph"
            app:layout_column="2"
            app:layout_row="1" />

        <Button
            android:id="@+id/button9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="setJosh"
            android:text="Josh"
            app:layout_column="3"
