            app:layout_row="3" />

        <Button
            android:id="@+id/button19"
            android:layout_width="114dp"
            android:layout_height="wrap_content"
            android:onClick="setSuwen"
            android:text="Suwen"
            app:layout_column="2"
            app:layout_row="0" />

        <Button
            android:id="@+id/button20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="setWhitney"
            android:text="Whitney"
            app:layout_column="1"
            app:layout_row="4" />

        <Button
            android:id="@+id/button21"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:onClick="setXue"
